{
  "tailwindCSS.includeLanguages": {
    "php": "html",
    "blade": "html",
    "vue": "html",
    "javascript": "html",
    "typescript": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    ["class\\s*=\\s*[\"']([^\"']*)[\"']", "[\"'`]([^\"'`]*)[\"'`]"],
    ["className\\s*=\\s*[\"']([^\"']*)[\"']", "[\"'`]([^\"'`]*)[\"'`]"],
    ["\\$.*class.*=.*[\"']([^\"']*)[\"']", "[\"'`]([^\"'`]*)[\"'`]"],
    ["@apply\\s+([^;\\n]+)", "([^\\s;\\n]+)"],
    ["(?:enter|leave)(?:From|To)?=\\s*(?:\"|')([^\"']*)(?:\"|')", "([^\"'\\s]+)"]
  ],
  "tailwindCSS.files.exclude": [
    "**/.git/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/vendor/**"
  ],
  "emmet.includeLanguages": {
    "php": "html",
    "blade": "html"
  },
  "files.associations": {
    "*.php": "php"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.validate": false,
  "scss.validate": false,
  "postcss.validate": false
}